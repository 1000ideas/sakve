if ( $('#edit-item-modal').length === 0) {
<% if params[:subaction] %>
var modal = sakve.modal('edit-item-modal',
    '<%= j render("items/edit/#{params[:subaction]}") %>');
<% else %>
var modal = sakve.modal('edit-item-modal',
    '<%= j render("form") %>');
<% end %>

modal
    .find('.tags-autocomplete')
    .autocomplete(sakve.defaults.tags_autocomplete)
}
