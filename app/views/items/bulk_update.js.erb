<%- if @ferror != true -%>
sakve.show_errors('#bulk-edit-modal',
  "<%= j render('l/partials/errors', errors: @ferror) %>");
var errors = $('<%= j render(partial: 'l/partials/errors', object: @ferror).strip.html_safe %>');
<%- elsif @ierror != true -%>
sakve.show_errors('#bulk-edit-modal',
  "<%= j render('l/partials/errors', errors: @ierror) %>");
<% else %>
sakve.reload_list('items')
$('#bulk-edit-modal').foundation('reveal', 'close');
<% end %>
