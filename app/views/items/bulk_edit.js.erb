if ( $('#bulk-edit-modal').length === 0) {
<% if params[:subaction] %>
var modal = sakve.modal('edit-bulk-modal',
    '<%= j render("items/bulk_edit/#{params[:subaction]}") %>');
<% else %>
var modal = sakve.modal('edit-bulk-modal',
    '<%= j render("form") %>');
<% end %>

modal
    .find('.tags-autocomplete')
    .autocomplete(sakve.defaults.tags_autocomplete)
}
