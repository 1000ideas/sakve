var form = $('#new_transfer');
<% if @transfer.errors.any? %>
var errors = $("<%= j render partial: 'l/partials/errors', object: @transfer.errors %>".trim());
var error_explanation = form.find('#error_explanation');
if (error_explanation.length == 0) {
  form.prepend( errors )
} else {
  error_explanation.replaceWith( errors );
}
<% else %>
var url = $('#transfers-list').data('url');
$('#transfers-list').load(url)
var dialog_box = form.parent();
dialog_box.dialog('close');
<%- @transfer = Transfer.new(user: current_user) -%>
dialog_box.html("<%= j render partial: 'form' %>");
$('.transfer-fileupload').each(function() {
  Sakve.transfer_init(this);
});
<% end %>

